<!doctype html>
<html>
  <head>
    <title>登录</title>
    <meta charset='utf-8'>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel='stylesheet' href='/css/login.css'>
    <link rel='stylesheet' href='/css/style.css'/>
    <link rel='stylesheet' href='/css/bootstrap.min.css'>
  </head>
  <body>
    <div>
      <div class="backimg">
        <div class="login-dialog bs-example bs-example-form">
          <div class="login-title">wsf</div>
          <div class="input-group group-style">
      			<span class="input-group-addon glyphicon glyphicon-user font-color"></span>
      			<input type="text" id="username" class="form-control" placeholder="用户名" required>
  		    </div>
          <div class="input-group group-style">
      			<span class="input-group-addon glyphicon glyphicon-lock font-color"></span>
      			<input type="password" id="password" class="form-control" placeholder="密码" required>
  		    </div>
          <button class="btn btn-default btn-block group-style" onclick="login()">登录</button>
        </div>
      </div>
    </div>
    <script type="text/javascript" src="/js/jquery.min.js"></script>
    <script type="text/javascript" src="/js/login/login.js"></script>
    <script src="/js/common/tips.js"></script>
  </body>
</html>
