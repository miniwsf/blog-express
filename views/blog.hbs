{{#each article}}
<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">
      {{title}}<span></span>
    </h3>
  </div>
  <div class="panel-body">
    <div>{{content}}</div>
    <div class="blog-info">
      <span class="glyphicon glyphicon-eye-open">{{type.typeName}}&nbsp;
      <span class="glyphicon glyphicon-eye-open"></span>{{readAmount}}&nbsp;
      <span class="glyphicon glyphicon-heart"></span>{{praiseNumber}}&nbsp;
      <span class="glyphicon glyphicon-time">{{create_time}}
    </div>
  </div>
</div>
{{/each}}
